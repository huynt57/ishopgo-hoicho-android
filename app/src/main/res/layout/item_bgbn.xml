<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/bg_white_black_bottom"
    android:foreground="?attr/selectableItemBackground"
    android:orientation="vertical"
    android:padding="@dimen/item_spacing_8dp"
    android:layout_marginBottom="8dp"
    tools:ignore="UnusedAttribute">

    <ishopgo.com.exhibition.ui.widget.VectorSupportTextView
        android:id="@+id/view_name"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:drawablePadding="@dimen/item_spacing_8dp"
        app:drawableStartCompat="@drawable/ic_search_shop" />

    <ishopgo.com.exhibition.ui.widget.VectorSupportTextView
        android:id="@+id/view_phone"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/item_spacing_8dp"
        android:drawablePadding="@dimen/item_spacing_8dp"
        app:drawableStartCompat="@drawable/ic_phone_highlight_24dp" />

</LinearLayout>