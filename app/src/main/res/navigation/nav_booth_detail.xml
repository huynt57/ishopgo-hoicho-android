<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            android:id="@+id/nav_booth_detail.xml"
            app:startDestination="@id/shopDetailFragmentActionBar">


    <fragment
        android:id="@+id/shopDetailFragmentActionBar"
        android:name="ishopgo.com.exhibition.ui.main.shop.ShopDetailFragmentActionBar"
        android:label="ShopDetailFragmentActionBar">
        <action
            android:id="@+id/action_shopDetailFragmentActionBar_self"
            app:destination="@id/shopDetailFragmentActionBar"
            app:enterAnim="@anim/nav_slide_right_to_left_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_pop_right_to_left_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"/>
    </fragment>
    <fragment
        android:id="@+id/qrCodeShopFragment"
        android:name="ishopgo.com.exhibition.ui.main.shop.qrcode.QrCodeShopFragment"
        android:label="QrCodeShopFragment">
        <action
            android:id="@+id/action_qrCodeShopFragment_to_shopDetailFragmentActionBar"
            app:destination="@id/shopDetailFragmentActionBar"
            app:enterAnim="@anim/nav_slide_right_to_left_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_pop_right_to_left_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"/>
    </fragment>
</navigation>