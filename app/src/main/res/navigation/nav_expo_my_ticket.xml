<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            android:id="@+id/nav_expo_my_ticket.xml"
            app:startDestination="@id/ticketFragmentActionBar">

    <fragment
        android:id="@+id/ticketFragmentActionBar"
        android:name="ishopgo.com.exhibition.ui.main.ticket.MyTicketFragmentActionBar"
        android:label="TicketFragmentActionBar">
        <action
            android:id="@+id/action_ticketFragmentActionBar_to_ticketDetailFragmentActionBar"
            app:destination="@id/ticketDetailFragmentActionBar"
            app:enterAnim="@anim/nav_slide_right_to_left_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_pop_right_to_left_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            />
    </fragment>
    <fragment
        android:id="@+id/ticketDetailFragmentActionBar"
        android:name="ishopgo.com.exhibition.ui.main.ticket.detail.TicketDetailFragmentActionBar"
        android:label="TicketDetailFragmentActionBar"/>
</navigation>