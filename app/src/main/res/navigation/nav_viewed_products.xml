<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            android:id="@+id/nav_viewed_products.xml"
            app:startDestination="@id/viewedProductsFragmentActionBar">

    <fragment
        android:id="@+id/viewedProductsFragmentActionBar"
        android:name="ishopgo.com.exhibition.ui.main.product.viewed.ViewedProductsFragmentActionBar"
        android:label="ViewedProductsFragmentActionBar">
        <action
            android:id="@+id/action_viewedProductsFragmentActionBar_to_searchViewedProductsFragment"
            app:destination="@id/searchViewedProductsFragment"
            app:enterAnim="@anim/nav_slide_right_to_left_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_pop_right_to_left_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"/>
        <action
            android:id="@+id/action_viewedProductsFragmentActionBar_to_filterProductFragment4"
            app:destination="@id/filterProductFragment4"
            app:enterAnim="@anim/nav_slide_right_to_left_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_pop_right_to_left_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"/>
        <action
            android:id="@+id/action_popularProductsFragmentActionBar_to_categoryFragment"
            app:destination="@id/categoryFragment"
            app:enterAnim="@anim/nav_slide_right_to_left_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_pop_right_to_left_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"/>
    </fragment>
    <fragment
        android:id="@+id/searchViewedProductsFragment"
        android:name="ishopgo.com.exhibition.ui.main.product.viewed.SearchViewedProductsFragment"
        android:label="SearchViewedProductsFragment"/>
    <fragment
        android:id="@+id/filterProductFragment4"
        android:name="ishopgo.com.exhibition.ui.filterproduct.FilterProductFragment"
        android:label="FilterProductFragment"/>
    <fragment
        android:id="@+id/categoryFragment"
        android:name="ishopgo.com.exhibition.ui.main.home.category.product.CategoryFragment"
        android:label="CategoryFragment" />
</navigation>